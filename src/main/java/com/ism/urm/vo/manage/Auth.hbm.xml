<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ism.urm.vo.manage.Auth" table="TB_USR_AUTH" entity-name="AUTH">
        <id name="id" type="string">
            <column name="AUTH_ID" /> <!-- pk -->
            <generator class="assigned" />
        </id>
        <property name="name" type="string" column="AUTH_NM" />
    </class> 
</hibernate-mapping>